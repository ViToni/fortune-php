    "version": "0.0.1",
    "type": "library",
    "keywords": [
        "fortune",
        "fortune-mod",
        "fortune-cookies"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Victor Toni",
            "email": "victor.toni@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
